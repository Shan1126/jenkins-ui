{
  "name": "jenkins-ui",
  "author": "Shan Gnanasambandham",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=2048 server.js",
    "prod": "ng build --prod=true --aot=true --baseHref=/jenkins-ui/",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage) /**/}*.{scss,html,ts,json}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "pre-commit": "npm run tslint:fix && npm run prettier",
    "tslint": "tslint --project tsconfig.json",
    "tslint:fix": "tslint --project tsconfig.json --fix",
    "stylelint:fix": "stylelint src/app/*.scss  --fix --syntax scss --config .stylelintrc.json",
    "postinstall": "ng build --prod --build-optimizer"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "all": "0.0.0",
    "compression": "^1.7.4",
    "core-js": "^2.5.4",
    "express": "^4.17.1",
    "http": "0.0.0",
    "i": "^0.3.6",
    "ngx-pagination": "^5.0.0",
    "ngx-spinner": "^7.2.0",
    "ngx-toastr": "^8.10.2",
    "node-sass": "^4.13.1",
    "npm": "^6.13.2",
    "path": "^0.12.7",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rxjs": "^6.5.3",
    "rxjs-compat": "^6.5.3",
    "stylelint": "^12.0.0",
    "stylelint-config-css-modules": "^2.1.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.13.0",
    "tslib": "^1.9.0",
    "tslint-config-prettier": "^1.18.0",
    "underscore": "^1.9.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.10",
    "@angular/cli": "~7.3.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "~3.2.2"
  }
}
